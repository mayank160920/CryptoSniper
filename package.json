{
  "name": "cryptosniper",
  "version": "1.1.4",
  "description": "CryptoSniper toolkit",
  "author": "blindgr2",
  "bin": {
    "CryptoSniper": "./bin/run"
  },
  "homepage": "https://github.com/blindgr2/CryptoSniper",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "blindgr2/CryptoSniper",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@mtproto/core": "6.2.0",
    "@oclif/command": "^1.8.16",
    "@oclif/core": "^1",
    "@oclif/plugin-autocomplete": "^1.2.0",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^2.0.1",
    "@oclif/plugin-warn-if-update-available": "^2.0.4",
    "abi-decoder": "^2.4.0",
    "bn.js": "^5.2.0",
    "chalk": "^4.1.2",
    "conf": "^10.1.2",
    "figlet": "^1.5.2",
    "inquirer": "^8.2.4",
    "node-emoji": "^1.11.0",
    "node-fetch": "^2.6.1",
    "ora": "^5.4.1",
    "readline": "^1.3.0",
    "web3": "^1.7.3",
    "winston": "^3.7.2",
    "winston-daily-rotate-file": "^4.5.5",
    "yarn": "^1.22.18"
  },
  "devDependencies": {
    "@oclif/test": "^2",
    "@types/chai": "^4",
    "@types/conf": "^3.0.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "^16.11.33",
    "@types/systeminformation": "^3.54.1",
    "chai": "^4",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.2",
    "ethereum-abi-types-generator": "^1.3.2",
    "globby": "^11",
    "mocha": "^9",
    "oclif": "^3.0.1",
    "pkg": "^5.8.0",
    "shx": "^0.3.3",
    "ts-node": "^10.2.1",
    "tslib": "^2.4.0",
    "typescript": "^4.6.4"
  },
  "oclif": {
    "bin": "CryptoSniper",
    "dirname": "CryptoSniper",
    "commands": "./dist/commands",
    "default": "start",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-autocomplete"
    ],
    "topicSeparator": " ",
    "hooks": {
      "init": "./src/hooks/init/configs"
    }
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md",
    "pkg": "pkg . --compress Brotli "
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "bugs": "https://github.com/blindgr2/CryptoSniper/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts",
  "pkg": {
    "assets": [
      "node_modules/figlet",
      "oclif.manifest.json"
    ],
    "targets": [
      "node18-linux-x64",
      "node18-macos-x64",
      "node18-win-x64",
      "node18-linux-arm64",
      "node18-macos-arm64"
    ],
    "outputPath": "pkg",
    "scripts": [
      "./dist/**/*.js"
    ]
  }
}
